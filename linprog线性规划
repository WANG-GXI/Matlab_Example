maxz=2x1+3x2−5x3
s.t  x1+x2+x3=7
     2x1-5x2+x3>=10
     x1+3x2+x3<=12
     x1,x2,x3>=0
%%%%%
linprog 只能求解最小值，在求解最大值时应把系数乘以-1
%%%%%

c = [2;3;-5];              %目标函数的系数
a = [-2,5,-1;1,3,1];       %不等式的系数（其中的不等式是小于等于0
b = [-10,12];              %不等式的右边的矩阵
aeq = [1,1,1];             %等式部分的系数
deq = 7;                   %等式的右边的值
x = linprog(-c,a,b,aeq,deq,zeros(3,1))
value = c'*x
